{
    "rules": {
        "indent": [
            1,
            "tab"
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "windows"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    }, "parserOptions": {
    "sourceType": "module"
},
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "react"
    ]
}